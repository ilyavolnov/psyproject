{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/[...path]"
    }
  ],
  "redirects": [
    {
      "source": "/admin-panel",
      "destination": "/admin-panel.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin-login.html"
    }
  ],
  "functions": {
    "api/[...path].js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
